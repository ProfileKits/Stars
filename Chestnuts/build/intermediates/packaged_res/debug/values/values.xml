    <dimen name="pickerview_topbar_paddingright">20dp</dimen>
    <dimen name="pickerview_topbar_title_textsize">17sp</dimen>
    <integer name="animation_default_duration">300</integer>
    <string name="data_exception">数据异常，请稍后重试</string>
    <string name="net_exception">网络异常，请稍后重试</string>
    <string name="pickerview_cancel">取消</string>
    <string name="pickerview_day">日</string>
    <string name="pickerview_hours">时</string>
    <string name="pickerview_minutes">分</string>
    <string name="pickerview_month">月</string>
    <string name="pickerview_seconds">秒</string>
    <string name="pickerview_submit">确认</string>
    <string name="pickerview_year">年</string>
    <string name="tip_permission_camera">·照相权限，用于拍摄功能</string>
    <string name="tip_permission_header">请允许APP获得以下权限</string>
    <string name="tip_permission_location">·定位权限，用于发送位置</string>
    <string name="tip_permission_phone">·手机状态权限，仅获取设备的状态，不监控通话记录</string>
    <string name="tip_permission_storage">·存储权限，用于保存文件</string>
    <string name="tip_permission_voice">·录音权限，用于发送语音</string>
    <string name="tip_unkown_error_place_holder">未知异常，%s</string>
    <declare-styleable name="FlikerProgressBar">
        <attr format="color" name="loadingColor"/>
        <attr format="color" name="stopColor"/>
        <attr format="color" name="finishColor"/>
        <attr format="dimension" name="radius"/>
        <attr format="dimension" name="borderWidth"/>
    </declare-styleable>
    <declare-styleable name="FontTextView">
        <attr format="string" name="font_name"/>
        <attr format="boolean" name="has_use"/>
        <attr format="boolean" name="has_Relaxation"/>
        <attr format="dimension" name="font_size_before"/>
        <attr format="dimension" name="font_size_after"/>
    </declare-styleable>
    <declare-styleable name="RealtimeBlurView">
        <attr format="dimension" name="realtimeBlurRadius"/>
        <attr format="float" name="realtimeDownsampleFactor"/>
        <attr format="color" name="realtimeOverlayColor"/>
    </declare-styleable>
    <declare-styleable name="RoundedRectangleBlurView">
        <attr format="dimension" name="roundedRectangleRadius"/>
    </declare-styleable>
    <declare-styleable name="cleanEditText">
        <attr format="reference" name="icon"/>
        <attr format="reference" name="focusIcon"/>
        <attr format="reference" name="deleteIcon"/>
        <attr format="dimension" name="iconWidth"/>
        <attr format="dimension" name="iconHeight"/>
        <attr format="dimension" name="deleteIconWidth"/>
        <attr format="dimension" name="deleteIconHeight"/>
        <attr format="color" name="dividerColor"/>
        <attr format="dimension" name="dividerBonds"/>
        <attr format="color" name="focusDividerColor"/>
        <attr format="dimension" name="dividerMarginLeft"/>
        <attr format="dimension" name="dividerMarginRight"/>
        <attr format="color" name="focusTextColor"/>
        <attr format="reference" name="background"/>
        <attr format="reference" name="cursor"/>
    </declare-styleable>
    <declare-styleable name="pickerview">
        <attr name="pickerview_gravity">
            <enum name="center" value="17"/>
            <enum name="left" value="3"/>
            <enum name="right" value="5"/>
        </attr>
        <attr format="dimension" name="pickerview_textSize"/>
        <attr format="color" name="pickerview_textColorOut"/>
        <attr format="color" name="pickerview_textColorCenter"/>
        <attr format="color" name="pickerview_dividerColor"/>
    </declare-styleable>
</resources>