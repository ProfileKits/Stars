<?xml version="1.0" encoding="utf-8"?>
<resources>
    <bool name="pickerview_customTextSize">true</bool>
    <color name="bgColor_overlay">#60000000</color>
    <color name="black">#FF000000</color>
    <color name="gray">#8f8f8f</color>
    <color name="green">#51B243</color>
    <color name="pickerview_bg_topbar">#f5f5f5</color>
    <color name="pickerview_timebtn_nor">#057dff</color>
    <color name="pickerview_timebtn_pre">#c2daf5</color>
    <color name="pickerview_topbar_title">#000000</color>
    <color name="pickerview_wheelview_textcolor_center">#2a2a2a</color>
    <color name="pickerview_wheelview_textcolor_divider">#d5d5d5</color>
    <color name="pickerview_wheelview_textcolor_out">#a8a8a8</color>
    <color name="teal_200">#FF03DAC5</color>
    <color name="teal_700">#FF018786</color>
    <color name="transparent">#00000000</color>
    <color name="white">#FFFFFFFF</color>
    <dimen name="pickerview_textsize">18sp</dimen>
    <dimen name="pickerview_topbar_btn_textsize">16sp</dimen>
    <dimen name="pickerview_topbar_height">55dp</dimen>
    <dimen name="pickerview_topbar_paddingleft">20dp</dimen>
    <dimen name="pickerview_topbar_paddingright">20dp</dimen>
    <dimen name="pickerview_topbar_title_textsize">17sp</dimen>
    <dimen name="scale_textSize">9sp</dimen>
    <integer name="animation_default_duration">300</integer>
    <string name="data_exception">数据异常，请稍后重试</string>
    <string name="net_exception">网络异常，请稍后重试</string>
    <string name="pickerview_cancel">取消</string>
    <string name="pickerview_day">日</string>
    <string name="pickerview_hours">时</string>
    <string name="pickerview_minutes">分</string>
    <string name="pickerview_month">月</string>
    <string name="pickerview_seconds">秒</string>
    <string name="pickerview_submit">确认</string>
    <string name="pickerview_year">年</string>
    <string name="tip_permission_camera">·照相权限，用于拍摄功能</string>
    <string name="tip_permission_header">请允许APP获得以下权限</string>
    <string name="tip_permission_location">·定位权限，用于发送位置</string>
    <string name="tip_permission_phone">·手机状态权限，仅获取设备的状态，不监控通话记录</string>
    <string name="tip_permission_storage">·存储权限，用于保存文件</string>
    <string name="tip_permission_voice">·录音权限，用于发送语音</string>
    <string name="tip_unkown_error_place_holder">未知异常，%s</string>
    <style name="dialog_style">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <declare-styleable name="FlikerProgressBar">
        <attr format="color" name="loadingColor"/>
        <attr format="color" name="stopColor"/>
        <attr format="color" name="finishColor"/>
        <attr format="dimension" name="radius"/>
        <attr format="dimension" name="borderWidth"/>
    </declare-styleable>
    <declare-styleable name="FontTextView">
        <attr format="string" name="font_name"/>
        <attr format="boolean" name="has_use"/>
        <attr format="boolean" name="has_Relaxation"/>
        <attr format="dimension" name="font_size_before"/>
        <attr format="dimension" name="font_size_after"/>
    </declare-styleable>
    <declare-styleable name="RealtimeBlurView">
        <attr format="dimension" name="realtimeBlurRadius"/>
        <attr format="float" name="realtimeDownsampleFactor"/>
        <attr format="color" name="realtimeOverlayColor"/>
    </declare-styleable>
    <declare-styleable name="RoundedRectangleBlurView">
        <attr format="dimension" name="roundedRectangleRadius"/>
    </declare-styleable>
    <declare-styleable name="cleanEditText">
        <attr format="reference" name="icon"/>
        <attr format="reference" name="focusIcon"/>
        <attr format="reference" name="deleteIcon"/>
        <attr format="dimension" name="iconWidth"/>
        <attr format="dimension" name="iconHeight"/>
        <attr format="dimension" name="deleteIconWidth"/>
        <attr format="dimension" name="deleteIconHeight"/>
        <attr format="color" name="dividerColor"/>
        <attr format="dimension" name="dividerBonds"/>
        <attr format="color" name="focusDividerColor"/>
        <attr format="dimension" name="dividerMarginLeft"/>
        <attr format="dimension" name="dividerMarginRight"/>
        <attr format="color" name="focusTextColor"/>
        <attr format="reference" name="background"/>
        <attr format="reference" name="cursor"/>
    </declare-styleable>
    <declare-styleable name="pickerview">
        <attr name="pickerview_gravity">
            <enum name="center" value="17"/>
            <enum name="left" value="3"/>
            <enum name="right" value="5"/>
        </attr>
        <attr format="dimension" name="pickerview_textSize"/>
        <attr format="color" name="pickerview_textColorOut"/>
        <attr format="color" name="pickerview_textColorCenter"/>
        <attr format="color" name="pickerview_dividerColor"/>
    </declare-styleable>
    <declare-styleable name="progress_circle">
        <attr format="color" name="text_color"/>
        <attr format="dimension" name="number_text_size"/>
        <attr format="dimension" name="unit_text_size"/>
        <attr format="dimension" name="progress_background_line_width"/>
        <attr format="dimension" name="progress_circle_line_width"/>
        <attr format="color" name="progress_background_line_color"/>
        <attr format="color" name="progress_circle_line_color"/>
        <attr format="string" name="unit"/>
    </declare-styleable>
    <declare-styleable name="verify_EditText">
        
        <attr format="integer" name="verify_count"/>
        
        <attr format="dimension" name="verify_width"/>
        
        <attr format="dimension" name="verify_height"/>
        
        <attr format="dimension" name="verify_margin"/>
        
        <attr format="dimension" name="verify_textSize"/>
        
        <attr format="color" name="verify_textColor"/>
        
        <attr format="reference" name="verify_background_normal"/>
        
        <attr format="reference" name="verify_background_selected"/>
        
        <attr format="boolean" name="verify_password"/>
        
        <attr format="boolean" name="verify_keyboard"/>
        
        <attr format="integer" name="verify_password_visible_time"/>
        
        <attr name="verify_inputType">
            <flag name="none" value="0x00000000"/>
            <flag name="text" value="0x00000001"/>
            <flag name="textCapCharacters" value="0x00001001"/>
            <flag name="textCapWords" value="0x00002001"/>
            <flag name="textCapSentences" value="0x00004001"/>
            <flag name="textAutoCorrect" value="0x00008001"/>
            <flag name="textAutoComplete" value="0x00010001"/>
            <flag name="textMultiLine" value="0x00020001"/>
            <flag name="textImeMultiLine" value="0x00040001"/>
            <flag name="textNoSuggestions" value="0x00080001"/>
            <flag name="textUri" value="0x00000011"/>
            <flag name="textEmailAddress" value="0x00000021"/>
            <flag name="textEmailSubject" value="0x00000031"/>
            <flag name="textShortMessage" value="0x00000041"/>
            <flag name="textLongMessage" value="0x00000051"/>
            <flag name="textPersonName" value="0x00000061"/>
            <flag name="textPostalAddress" value="0x00000071"/>
            <flag name="textPassword" value="0x00000081"/>
            <flag name="textVisiblePassword" value="0x00000091"/>
            <flag name="textWebEditText" value="0x000000a1"/>
            <flag name="textFilter" value="0x000000b1"/>
            <flag name="textPhonetic" value="0x000000c1"/>
            <flag name="textWebEmailAddress" value="0x000000d1"/>
            <flag name="textWebPassword" value="0x000000e1"/>
            <flag name="number" value="0x00000002"/>
            <flag name="numberSigned" value="0x00001002"/>
            <flag name="numberDecimal" value="0x00002002"/>
            <flag name="numberPassword" value="0x00000012"/>
            <flag name="phone" value="0x00000003"/>
            <flag name="datetime" value="0x00000004"/>
            <flag name="date" value="0x00000014"/>
            <flag name="time" value="0x00000024"/>
        </attr>
    </declare-styleable>
</resources>